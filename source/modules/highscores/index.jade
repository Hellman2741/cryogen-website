extend ../default

block content
  div.container(style='min-height: 700px;')
    div.skills
      -for i in jIterator.iterate(25)
        a.skill(skill_id=''+i, href='#')
          img(src='images/skills_hd/'+i+'.png')
    div.widget(style='min-height: 400px;')
      div.header
        h4.color-white Highscores - Global
      div.cnt
        table.table.hs-table.table-condensed.table-responsive
          thead
            tr
              th Rank
              th Username
              th Total Level
              th Total XP
          tbody
            -for user in hsusers.getList()
              tr
                td.b-right.rank= user.getRank()
                td.b-right= user.getName()
                td.b-right= user.getTotalLevel()
                td= user.getTotalXP()
